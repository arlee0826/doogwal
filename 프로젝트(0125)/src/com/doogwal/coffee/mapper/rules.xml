<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC	
"-//mybatis.org//DTD Mapper 3.0//EN"	
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="rules">
<!-- 송진현 -->
<select id="selectList" parameterType="int" resultType="Rule">
		SELECT rule
		FROM rules
		WHERE meeting_no=#{no}
	</select>
	<!-- end 송진현 -->
	


</mapper>  
	
